package maths;

public class Maths {
	public static void main(String[] args) {
		System.out.println(sum(6, 12));
		System.out.println(multiply(6, 12));
		System.out.println(subtract(6, 12));
		System.out.println(divide(6, 12));
		
	}
	private static int sum(int a, int b) {
		return a + b;
	}
	private static int multiply(int c, int d) {
		return c*d;
	}
	private static int subtract(int e, int f) {
		return e-f;
	}
	private static double divide(int g, int h) {
		double answer=g/h;
		return answer;
	}
	
}
