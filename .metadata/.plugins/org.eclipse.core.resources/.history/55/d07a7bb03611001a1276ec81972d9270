package app;

import results.Results;

public class App 
{
    public static void main( String[] args ) {
    Results myResults = new Results();
    int phyGrade = 55;
    int chemGrade = 60;
    int bioGrade = 30;
    myResults.display(phyGrade, chemGrade, bioGrade);
    myResults.totalPercentage(phyGrade, chemGrade, bioGrade);
    myResults.failCheck(phyGrade, chemGrade, bioGrade);
    
    
    }
}
